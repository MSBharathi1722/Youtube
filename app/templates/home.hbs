{{!-- template-lint-disable require-valid-alt-text --}}
<div class="videos">
    <section class="video-section">
      {{#each this.result as |item|}}
        <article class="video-container">
          <LinkTo @route="home">
            <a href="#" class="thumbnail" data-duration="12:24">
              <img class="thumbnail-image" src="{{item.snippet.thumbnails.high.url}}" />
            </a>
            <div class="video-bottom-section">
              <a href="#">
                <img class="channel-icon" src="http://unsplash.it/36/36?gravity=center" />
              </a>
              <div class="video-details">
                <a href="#" class="video-title">{{item.snippet.title}}</a>
                <a href="#" class="video-channel-name">Channel Name</a>
                <div class="video-metadata">
                  <span>12K views</span>
                  •
                  <span>1 week ago</span>
                </div>
              </div>
            </div>
          </LinkTo>

        </article>
      {{/each}}
    </section>
