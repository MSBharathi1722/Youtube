    <form class="search-bar">
      <Input class="search-input" @value={{this.query}} @type="search" placeholder="Search" aria-label="Search" />
      {{!-- template-lint-disable no-action --}}
      <button type="submit" {{action this.search}} class="search-btn">
        <img src="magnify.svg" />
      </button>
    </form>
